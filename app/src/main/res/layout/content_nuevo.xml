<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="unid.arturo.autos.Nuevo"
    tools:showIn="@layout/activity_nuevo">

    <EditText
        android:id="@+id/txt_marca"
        android:layout_width="300dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView"
        android:layout_marginStart="27dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/txt_marca"
        android:layout_marginTop="36dp"
        android:text="Marca"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/txt_modelo"
        android:layout_width="300dp"
        android:layout_height="41dp"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_marca"
        android:layout_below="@+id/txt_marca"
        android:text="Modelo"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="16dp" />


    <EditText
        android:id="@+id/txt_tipo"
        android:layout_width="300dp"
        android:layout_height="41dp"
        android:layout_alignStart="@+id/txt_modelo"
        android:layout_below="@+id/txt_modelo"
        android:layout_marginTop="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_tipo"
        android:layout_below="@+id/txt_modelo"
        android:layout_marginTop="12dp"
        android:text="Tipo"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/txt_inventario"
        android:layout_width="300dp"
        android:layout_height="41dp"
        android:layout_alignStart="@+id/txt_tipo"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="13dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_inventario"
        android:layout_below="@+id/txt_tipo"
        android:layout_marginTop="12dp"
        android:text="Inventario"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/txt_precio"
        android:layout_width="300dp"
        android:layout_height="41dp"
        android:layout_alignStart="@+id/txt_inventario"
        android:layout_below="@+id/txt_inventario"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_precio"
        android:layout_below="@+id/txt_inventario"
        android:layout_marginTop="21dp"
        android:text="Precio"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="16dp" />

</RelativeLayout>
